
<% include ../partials/header %>
<div class="page-content">
        <section class="top-courses top-masterclasses">
          <div class="link-prev-courses"><%=portfolio.host%></div>
          <div class="link-next-courses"><%=moment(portfolio.date).format('DD/MM/YYYY')%></div>
          <div class="top-courses-image-inner">
            <div class="image">
              <img class="inner-image" src="<%=portfolio.image%>">
            </div>
            <div class="top-courses-title top-masterclasses-title " data-parallax="0.2">
              <h1>
                <span class="span-outer" style="width: 1000px"><span class="span-inner title" style="width: 1000px"><%=portfolio.name%></span></span>
                <!--<span class="span-outer" style="width: 520px"><span class="span-inner" style="width: 520px">Gillot</span></span>-->
              </h1>
            </div>
            <div class="top-courses-year " data-parallax="0.15">Date: <%=moment(portfolio.date).format('L')%></div>
          </div>
          <div class="top-courses-info top-masterclasses-info">
            <div class="top-courses-info-inner portfolio-date">
              <div class="top-courses-desc"> Our Service:
                <% if (portfolio.numberofservice === 4){ %>
                <%=portfolio.service1%>, <%=portfolio.service2%>, <%=portfolio.service3%>, <%=portfolio.service4%>
                <% } else if (portfolio.numberofservice ===3){ %>
                <%=portfolio.service1%>, <%=portfolio.service2%>, <%=portfolio.service3%>
                <% } else if (portfolio.numberofservice ===2){ %>
                <%=portfolio.service1%>, <%=portfolio.service2%>
                <% } else if (portfolio.numberofservice ===1){ %>
                <%=portfolio.service1%>
                <% } %>
                </div>
            </div>
            <div class="bg" data-parallax="0.3"></div>
          </div>
        </section>

        <section class="home1">
          <p data-parallax="0.2" class="portfolio-p"><%=portfolio.desc%></p>
        </section>

        <div class="container">
        <div class="row">
          <div class="col-md-10 col-md-offset-1 col-xs-12 col-xs-offset-0 nopadding">
            <% if (!portfolio.video){ %>
            </div>
          </div>
          </div>
            <% } else { %>
            <div class="embed-responsive embed-responsive-16by9">
              <iframe id="ytiframe" class="embed-responsive-item" width="854" height="480" src="<%=portfolio.video%>?&autohide=2&rel=0" frameborder="0" allowfullscreen></iframe>
            </div>
            <br>
          </div>
        </div>
        </div>
        <% } %>

        <div class="container carousel">
        <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">

        <!-- Wrapper for slides -->
        <div class="carousel-inner" role="listbox">
          <div class="item active">
            <img src="<%=portfolio.image%>">
            <div class="carousel-caption">
            </div>
          </div>
          <div class="item">
            <img src="<%=portfolio.image1%>">
            <div class="carousel-caption">
            </div>
          </div>
          <div class="item">
            <img src="<%=portfolio.image2%>">
            <div class="carousel-caption">
            </div>
          </div>
          <div class="item">
            <img src="<%=portfolio.image3%>">
            <div class="carousel-caption">
            </div>
          </div>
          <div class="item">
            <img src="<%=portfolio.image4%>">
            <div class="carousel-caption">
            </div>
          </div>
          <div class="item">
            <img src="<%=portfolio.image5%>">
            <div class="carousel-caption">
            </div>
          </div>
          <div class="item">
            <img src="<%=portfolio.image6%>">
            <div class="carousel-caption">
            </div>
          </div>
          <div class="item">
            <img src="<%=portfolio.image7%>">
            <div class="carousel-caption">
            </div>
          </div>
          <div class="item">
            <img src="<%=portfolio.image8%>">
            <div class="carousel-caption">
            </div>
          </div>
          <div class="item">
            <img src="<%=portfolio.image9%>">
            <div class="carousel-caption">
            </div>
          </div>
          <div class="item">
            <img src="<%=portfolio.image10%>">
            <div class="carousel-caption">
            </div>
          </div>
        </div>

        <!-- Controls -->
        <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
          <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
          <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>

      </div>
    </div>
    <div class="container">
      <span><a href="javascript:history.back()" class="back" style="position:relative">BACK</a></span>
    </div>
</div>
<!-- END PAGE CONTENT -->

<% include ../partials/footer %>
